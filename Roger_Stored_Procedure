--SP for tblEmployee
CREATE PROCEDURE jakrINSERTEMPLOYEE
@Gender varchar (20),
@EFN VARCHAR (20),
@ELN VARCHAR (20),
@EC VARCHAR (50),
@ES VARCHAR (50),
@EZP INT(20),
@DOB DATE,
@HD DATE
AS
DECLARE @EMP_ID INT

SET @EMP_ID = (SELECT GenderID from tblGENDER WHERE Gender = @Gender)

BEGIN TRAN G1
INSERT INTO tblEmployee (GenderID, EmployeeFName, EmployeeLName, EmployeeCity, EmployeeState, EmployeeZipCode, DateOfBirth, HiredDate)
VALUES (@EFN,@ELN,@EC,@ES,@EZP,@DOB,@HD)

COMMIT TRAN G1

